#!/bin/bash

set -e # exit immediately if a simple command exits with a non-zero status
set -u # report the usage of uninitialized variables

# Setup env vars and folders for the webapp_ctl script
source /var/vcap/jobs/publish_autosleep/helpers/ctl_setup.sh 'publish_autosleep'

/var/vcap/jobs/publish_autosleep/bin/errand.sh

EXITSTATUS=0

echo "Errand publish_autosleep is complete; exit status $EXITSTATUS"
exit $EXITSTATUS
